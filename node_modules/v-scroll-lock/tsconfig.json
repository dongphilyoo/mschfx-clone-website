{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "declaration": true,
    "types": ["vue", "node"],
    "lib": ["es5", "es6", "es7", "dom"]
  },
  "exclude": ["node_modules"]
}
